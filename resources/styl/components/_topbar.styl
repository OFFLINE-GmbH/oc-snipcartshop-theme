.topbar
    background #f3f3f3
    lost-utility clearfix
    padding 1em 0 .9em
    font-size .8em
    line-height 1
    text-transform uppercase
    a
        color inherit

    span
        color primary-color

    &__left,
    &__center,
    &__right
        lost-column 1/3

    &__center
        text-align center

    &__right
        text-align right
